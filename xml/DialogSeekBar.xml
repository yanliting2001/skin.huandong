<?xml version="1.0" encoding="UTF-8"?>
<window>
	<defaultcontrol>401</defaultcontrol>
	<visible>Window.IsVisible(FullScreenVideo)</visible>
	<visible>[Player.Seeking | Player.DisplayAfterSeek | Player.Paused | Player.Forwarding | Player.Rewinding] + ![Window.IsActive(videoosd) | Window.IsActive(1122)]</visible>
	<animation effect="fade" start="0" end="100" time="200">WindowOpen</animation>
	<animation effect="fade" start="100" end="0" time="200">WindowClose</animation>
	<controls>
		<control type="group">
			<!-- Top group (Title + Time) -->
			<animation effect="slide" start="0,-132" end="0,0" time="200">WindowOpen</animation>
