			<animation effect="slide" start="0,-132" end="0,0" time="200">VisibleChange</animation>
			<height>132</height>
			<control type="image">
				<width>100%</width>
				<height>132</height>
				<texture>player/osd_bg.png</texture>
			</control>
			<control type="grouplist">
				<left>50</left>
				<centertop>50%</centertop>
				<right>300</right>
				<height>50</height>
				<itemgap>20</itemgap>
				<usecontrolcoords>true</usecontrolcoords>
				<orientation>horizontal</orientation>
				<control type="label">
					<width>auto</width>
					<height>50</height>
					<font>font40</font>
					<aligny>center</aligny>
					<label>[B]$INFO[Player.Title][/B]</label>
